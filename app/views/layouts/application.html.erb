<!DOCTYPE html>
<html>
  <head>
    <title>Talisman</title>
    <%= csrf_meta_tags %>
    <link href="//cdn.muicss.com/mui-0.9.9-rc2/css/mui.min.css" rel="stylesheet" type="text/css" />
	<script src="//cdn.muicss.com/mui-0.9.9-rc2/js/mui.min.js"></script>
	<link href="https://fonts.googleapis.com/css?family=Dancing+Script|Playfair+Display|Quicksand|Raleway|Roboto+Slab" rel="stylesheet"> 

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <%=render 'layouts/side_drawer' %>
  <%=render 'layouts/header' %>
  <%if flash.notice || flash.alert  %>
  <div class="mui-panel">
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <%else%>
  <p> </p>
  <%end%>
</div>
    <%= yield %>
  </body>
  <%= debug(params) if Rails.env.development? %>

  <%= render 'layouts/footer' %>
</html>
